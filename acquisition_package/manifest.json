{
  "manifest_version": "2.0",
  "system_name": "DoD Acquisition Contract Package Automation",
  "description": "Master manifest for 28 required acquisition documents supporting USMC and Space Force contract award packages. Designed for multi-agent orchestration with cross-referencing, dependency resolution, and threshold-based routing.",
  "applicable_services": ["USMC", "USSF", "USN", "USAF", "DoD-Wide"],
  "regulatory_framework": {
    "primary": ["FAR", "DFARS"],
    "service_supplements": {
      "USMC": "NMCARS (Navy Marine Corps Acquisition Regulation Supplement)",
      "USSF": "AFFARS (Air Force Federal Acquisition Regulation Supplement)",
      "USN": "NMCARS",
      "USAF": "AFFARS"
    }
  },
  "thresholds": {
    "micro_purchase": 10000,
    "simplified_acquisition": 250000,
    "sealed_bidding": 250000,
    "certified_cost_data": 2000000,
    "acquisition_plan_required": 50000000,
    "congressional_notification": 25000000,
    "subcontracting_plan_required": 750000,
    "subcontracting_plan_construction": 1500000,
    "synopsis_required": 25000,
    "j_and_a_approval_tiers": {
      "contracting_officer": 750000,
      "head_of_contracting_activity": 15000000,
      "senior_procurement_executive": 100000000,
      "agency_head_or_designee": "above_100000000"
    }
  },
  "execution_phases": [
    {
      "phase": 1,
      "name": "Requirements Definition",
      "description": "Define the capability need and develop the requirements documentation",
      "documents": ["DOC-006", "DOC-017"]
    },
    {
      "phase": 2,
      "name": "Market Research & Planning",
      "description": "Investigate market, estimate costs, develop acquisition strategy",
      "documents": ["DOC-002", "DOC-003", "DOC-001"]
    },
    {
      "phase": 3,
      "name": "Pre-Solicitation Approvals",
      "description": "Obtain required approvals based on competition strategy and contract type",
      "documents": ["DOC-004", "DOC-005", "DOC-013", "DOC-014", "DOC-023", "DOC-016"]
    },
    {
      "phase": 4,
      "name": "Solicitation Development",
      "description": "Build the solicitation package including evaluation criteria and source selection plan",
      "documents": ["DOC-007", "DOC-009", "DOC-008", "DOC-019"]
    },
    {
      "phase": 5,
      "name": "Evaluation & Award",
      "description": "Evaluate proposals, negotiate, and make award determination",
      "documents": ["DOC-010", "DOC-011", "DOC-012", "DOC-015", "DOC-022"]
    },
    {
      "phase": 6,
      "name": "Award & Post-Award",
      "description": "Execute contract, distribute, notify, and establish administration",
      "documents": ["DOC-018", "DOC-020", "DOC-024", "DOC-025", "DOC-026", "DOC-027", "DOC-028"]
    },
    {
      "phase": 7,
      "name": "Contract Administration (Ongoing)",
      "description": "Ongoing performance monitoring and evaluation",
      "documents": ["DOC-021"]
    }
  ],
  "documents": [
    {
      "document_id": "DOC-001",
      "title": "Acquisition Strategy / Acquisition Plan",
      "short_name": "acq_strategy",
      "schema_file": "schemas/DOC-001_acquisition_strategy.json",
      "prompt_file": "prompts/DOC-001_acquisition_strategy.md",
      "phase": 2,
      "required": true,
      "conditional_requirement": "Always required; scope/depth varies by dollar threshold and ACAT level",
      "depends_on": ["DOC-002", "DOC-003", "DOC-006"],
      "feeds_into": ["DOC-004", "DOC-007", "DOC-008", "DOC-016"],
      "data_sources": {
        "from_DOC-002": ["commercial_availability", "capable_sources_count", "recommended_approach", "recommended_contract_type"],
        "from_DOC-003": ["total_estimated_cost", "cost_by_period"],
        "from_DOC-006": ["requirement_summary", "period_of_performance", "security_level"]
      }
    },
    {
      "document_id": "DOC-002",
      "title": "Market Research Report",
      "short_name": "market_research",
      "schema_file": "schemas/DOC-002_market_research.json",
      "prompt_file": "prompts/DOC-002_market_research.md",
      "phase": 2,
      "required": true,
      "conditional_requirement": "Always required per FAR Part 10",
      "depends_on": ["DOC-006"],
      "feeds_into": ["DOC-001", "DOC-003", "DOC-004", "DOC-013"],
      "data_sources": {
        "from_DOC-006": ["requirement_description", "key_performance_parameters"]
      }
    },
    {
      "document_id": "DOC-003",
      "title": "Independent Government Cost Estimate (IGCE)",
      "short_name": "igce",
      "schema_file": "schemas/DOC-003_igce.json",
      "prompt_file": "prompts/DOC-003_igce.md",
      "phase": 2,
      "required": true,
      "conditional_requirement": "Always required; depth varies by contract type",
      "depends_on": ["DOC-002", "DOC-006"],
      "feeds_into": ["DOC-001", "DOC-004", "DOC-008", "DOC-012", "DOC-017"],
      "data_sources": {
        "from_DOC-002": ["price_range", "labor_rate_data", "commercial_pricing"],
        "from_DOC-006": ["tasks", "deliverables", "period_of_performance", "labor_categories"]
      }
    },
    {
      "document_id": "DOC-004",
      "title": "Justification & Approval (J&A)",
      "short_name": "j_and_a",
      "schema_file": "schemas/DOC-004_justification_approval.json",
      "prompt_file": "prompts/DOC-004_justification_approval.md",
      "phase": 3,
      "required": false,
      "conditional_requirement": "Required ONLY when using other-than-full-and-open competition (FAR 6.3)",
      "trigger_condition": "competition_strategy != 'full_and_open'",
      "depends_on": ["DOC-001", "DOC-002", "DOC-003", "DOC-006"],
      "feeds_into": ["DOC-008", "DOC-016"],
      "data_sources": {
        "from_DOC-001": ["competition_strategy", "contract_type"],
        "from_DOC-002": ["capable_sources_count", "market_research_summary"],
        "from_DOC-003": ["total_estimated_cost"],
        "from_DOC-006": ["requirement_description"]
      }
    },
    {
      "document_id": "DOC-005",
      "title": "Determination & Findings (D&F)",
      "short_name": "d_and_f",
      "schema_file": "schemas/DOC-005_determination_findings.json",
      "prompt_file": "prompts/DOC-005_determination_findings.md",
      "phase": 3,
      "required": false,
      "conditional_requirement": "Required for specific contract actions: T&M/LH contracts, use of other than sealed bidding, consent to subcontract, progress payments, etc.",
      "trigger_conditions": [
        "contract_type in ['T&M', 'LH']",
        "procurement_method != 'sealed_bidding' AND value > simplified_acquisition_threshold",
        "progress_payments_required == true",
        "contract_financing_required == true"
      ],
      "depends_on": ["DOC-001", "DOC-006"],
      "feeds_into": ["DOC-008", "DOC-016"],
      "data_sources": {
        "from_DOC-001": ["contract_type", "acquisition_approach"],
        "from_DOC-006": ["requirement_complexity", "performance_uncertainty"]
      }
    },
    {
      "document_id": "DOC-006",
      "title": "Performance Work Statement / Statement of Work / Statement of Objectives",
      "short_name": "pws_sow_soo",
      "schema_file": "schemas/DOC-006_pws_sow_soo.json",
      "prompt_file": "prompts/DOC-006_pws_sow_soo.md",
      "phase": 1,
      "required": true,
      "conditional_requirement": "Always required; document type (PWS vs SOW vs SOO) depends on acquisition approach",
      "depends_on": [],
      "feeds_into": ["DOC-001", "DOC-002", "DOC-003", "DOC-007", "DOC-008", "DOC-009", "DOC-019"],
      "data_sources": {}
    },
    {
